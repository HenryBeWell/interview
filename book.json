{
  "title": "Interview",
  "author": "HenryBeWell",
  "description" : "面试中的一些技术总结",
  "language" : "zh-hans",
  "extension": null,
  "generator": "site",
  "root": ".",
  "introduction": {
        "path": "README.md"
    },
  "plugins": [
    "-lunr",
    "-search",
    "search-pro",
    "-sharing",
    "disqus",
    "ga",
    "gtoc",
    "github-buttons",
    "editlink",
    "-highlight",
    "katex",
    "prism",
    "bibtex-indexed-cite",
    "anchor-navigation-ex",
    "page-footer-ex",
    "splitter",
    "copy-code-button",
    "expandable-chapters-small",
    "donate"
  ],
  "pluginsConfig": {
    "search-pro": {
      "cutWordLib": "nodejieba",
      "defineWord": ["Gitbook Use"]
    },
    "fontSettings": {
        "theme": "white",
        "family": "serif",
        "size": 2
        },
    "theme-default": {
      "showLevel": true
    },
	"editlink": {
      "base": "https://github.com/henrybewell/interview/blob/master",
      "label": "Edit",
      "multilingual": false
    },
    "anchor-navigation-ex": {
      "showLevel": false
    },
    "disqus": {
      "shortName": "interview"
    },
    "ga": {
      "token": "UA-139847070-1"
    },
    "github-buttons": {
      "buttons": [{
        "user": "henrybewell",
        "repo": "interview",
        "type": "star",
        "size": "small",
        "count": true
      }]
    },
	
    "prism": {
      "ignore": [
        "mermaid",
        "eval-js"
      ],
      "css": [
        "prismjs/themes/prism-solarizedlight.css"
      ]
    },
    "page-footer-ex": {
      "copyright": "By [henrybewell](https://github.com/henrybewell)，使用[知识共享 署名-相同方式共享 4.0协议](https://creativecommons.org/licenses/by-sa/4.0/)发布",
      "markdown": true,
      "update_label": "此页面修订于：",
      "update_format": "YYYY-MM-DD HH:mm:ss"
    },
    "bibtex-indexed-cite": {
      "path": "/"
    },
    "donate": {
      "wechat": "https://henrybewell.github.io/images/wechatpay.png",
      "title": "",
      "button": "赏",
      "wechatText": "微信打赏"
    }
  }
}